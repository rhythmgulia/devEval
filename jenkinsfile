pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('Build'){
            steps{
                sh 'mvn clean package'
                echo "build successful"
            }
        }
        stage('Test'){
            steps{
                sh 'mvn run build'
            }
        }
